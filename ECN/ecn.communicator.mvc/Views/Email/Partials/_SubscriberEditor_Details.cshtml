        <td>User1: </td>
        <td>@Html.TextBoxFor(e => e.User1)</td>
        <td>User Event1: </td>
        <td>@Html.TextBoxFor(e => e.UserEvent1)</td>
    </tr>
    <tr>
        <td>User2: </td>
        <td>@Html.TextBoxFor(e => e.User2)</td>
        <td>User Event1 Date [MM/DD/YYYY]:</td>
        <td>@Html.TextBoxFor(e => e.UserEvent1Date)</td>
    </tr>
    <tr>
        <td>User3: </td>
        <td>@Html.TextBoxFor(e => e.User3)</td>
        <td></td>
        <td></td>
    </tr>
    <tr>
        <td>User4: </td>
        <td>@Html.TextBoxFor(e => e.User4)</td>
