<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>ECN</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @Styles.Render("~/Content/css")
    </head>
    <body>
        <header>
            <div class="content-wrapper">
                <div class="float-left">
                    <table width="750" border="0" cellpadding="0" cellspacing="0" id="Table1">
                        <tr>
                            <td align="center" style="font-family: Arial, Helvetica, sans-serif; font-size: 10px;"></td>
                        <tr>
                            <td align="left" style="padding: 0px, 0px, 0px, 30px;"><a href="http://www.ecn5.com/index.htm">                <img src="http://www.ecn5.com/ecn.images/Channels/12/kmlogo.jpg" border="0"></a></td>
                            
                        </tr>
                    </table>
                </div>

            </div>
        </header>
        <div id="body">
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer>
            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year - Knowledge Marketing</p>
                </div>
            </div>
        </footer>
    </body>
</html>
