@model IEnumerable<FrameworkUAD.Object.UADError>
@if (Model.Count() > 0)
{
    <div class="alert alert-danger">
        <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
        <strong>Error!</strong>
        @foreach (var error in Model)
        {
            <div>@error.ErrorMessage</div>
        }
    </div>

}

