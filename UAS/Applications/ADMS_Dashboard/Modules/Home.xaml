                    </telerik:RadCartesianChart.HorizontalAxis>
                    <telerik:RadCartesianChart.VerticalAxis>
                        <telerik:LinearAxis LineStroke="#FFF47E1F"/>
                    </telerik:RadCartesianChart.VerticalAxis>
                </telerik:RadCartesianChart>
            </StackPanel>
            <Grid Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="350"/>
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <StackPanel x:Name="spDataBarsGraphs" Grid.Column="0" Grid.Row="0" Orientation="Vertical" VerticalAlignment="Top" Margin="55,10,10,0" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Viewing for: "  Margin="0,0,0,10"/>
                        <TextBlock x:Name="RegionName" FontStyle="Italic" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Width="300" Margin="0,0,0,5">
                        <TextBlock Text="Files Summary" FontFamily="Calibri" Margin="0,0,2,0" FontSize="13"/>
                        <StackPanel Orientation="Vertical" Margin="0,0,5,0">
                            <TextBlock Text="Completed " FontSize="8"/>
                            <Rectangle Height="4" Width="Auto" Fill="#acd47b" />
                        </StackPanel>
                        <StackPanel Orientation="Vertical" Margin="0,0,5,0">
                            <TextBlock Text="Run" FontSize="8"/>
                            <Rectangle Height="4" Width="Auto" Fill="#FF045DA4" />
                        </StackPanel>
                        <StackPanel Orientation="Vertical" Margin="0,0,5,0">
                            <TextBlock Text="Invalid" FontSize="8"/>
                            <Rectangle Height="4" Width="Auto" Fill="#FF5C33" />
                        </StackPanel>
                        <StackPanel Orientation="Vertical">
                            <TextBlock Text="Received" FontSize="8"/>
                            <Rectangle Height="4" Width="Auto" Fill="#FFF47E1F" />
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Orientation="Vertical">
                        <telerik:RadDataBar x:Name="rdbCompletedGraphs" Margin="0,0,0,-4" Height="15" ValueBrush="#acd47b" Value="70" Maximum="{Binding ElementName=rdbReceivedGraphs, Path=Value}"/>
                        <telerik:RadDataBar x:Name="rdbRunGraphs" Height="15" Margin="0,0,0,-4" Value="80" ValueBrush="#FF045DA4" Maximum="{Binding ElementName=rdbReceivedGraphs, Path=Value}"/>
                        <telerik:RadDataBar x:Name="rdbInvalidGraphs" Height="15" Margin="0,0,0,-4" Value="2" ValueBrush="#FF5C33" Maximum="{Binding ElementName=rdbReceivedGraphs, Path=Value}"/>
                        <telerik:RadDataBar x:Name="rdbReceivedGraphs" Height="15" Value="150" ValueBrush="#FFF47E1F" ShowOutOfRangeIndicators="False" />
                    </StackPanel>
                </StackPanel>
                <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Width="300" Margin="55,125,10,5" Grid.Column="0" Grid.Row="0" VerticalAlignment="Top">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Records Summary" FontFamily="Calibri" Margin="0,0,2,0" FontSize="13"/>
                        <StackPanel Orientation="Horizontal">
                            <StackPanel Orientation="Vertical" Margin="0,0,5,0">
                                <TextBlock Text="Actual" FontSize="8"/>
                                <Rectangle Height="4" Width="Auto" Fill="#FF045DA4" />
                            </StackPanel>
                            <TextBlock Text="vs  " FontSize="6" VerticalAlignment="Center"/>
                            <StackPanel Orientation="Vertical">
                                <TextBlock Text="Target" FontSize="8"/>
                                <Rectangle Height="4" Width="Auto" Fill="#FFF47E1F" />
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock x:Name="actualValueRecords" Foreground="#FF045DA4" Text="{Binding ElementName=rdbUADRecordsActual, Path=Value}" Margin="0,0,8,0"/>
                        <TextBlock x:Name="targetValueRecords" Foreground="#FFF47E1F" Text="{Binding ElementName=rdbUADRecordsTarget, Path=Value}" Margin="0,0,8,0"/>
                        <TextBlock x:Name="percentageCompleteRecords" />
                    </StackPanel>
                    <StackPanel Orientation="Vertical">
                        <telerik:RadDataBar x:Name="rdbUADRecordsActual" Margin="0,0,0,-8" Height="15" ValueBrush="#FF045DA4" Maximum="{Binding ElementName=rdbUADRecordsTarget, Path=Value}" Value="15000" ShowOutOfRangeIndicators="False" LabelVisibility="Hidden"/>
                        <telerik:RadDataBar x:Name="rdbUADRecordsTarget" Margin="0,0,0,-4" Height="15" ValueBrush="#FFF47E1F" Value="20000" BarHeightPercent=".2" LabelVisibility="Hidden"/>
                    </StackPanel>
                </StackPanel>
                <Border Grid.Column="1" Grid.Row="0" BorderBrush="Black" BorderThickness="1" Margin="10" Height="200" VerticalAlignment="Top">
                    <telerik:RadMap x:Name="RadMap1" Grid.Column="1" Grid.Row="0" Background="#D9E1FF" BorderBrush="Black" BorderThickness="1" UseDefaultLayout="False" MouseClickMode="SelectItem" MouseSelectionMode="RaiseEvent" 
                MouseDoubleClickMode="None" Center="65,-20" ZoomLevel="0" Tag="World" MaxZoomLevel="4" Height="200">
                        <telerik:RadMap.Provider>
                            <telerik:EmptyProvider />
                        </telerik:RadMap.Provider>
                        <telerik:VisualizationLayer x:Name="visualizationLayer" SelectionChanged="VisualizationLayer_SelectionChanged">
                            <telerik:VisualizationLayer.ShapeFill>
                                <telerik:MapShapeFill Fill="#FFFFF2" />
                            </telerik:VisualizationLayer.ShapeFill>
                            <telerik:VisualizationLayer.HighlightFill>
                                <telerik:MapShapeFill Fill="#80FFA500" />
                            </telerik:VisualizationLayer.HighlightFill>
                            <telerik:VisualizationLayer.SelectedFill>
                                <telerik:MapShapeFill Fill="#80208020" />
                            </telerik:VisualizationLayer.SelectedFill>
                            <telerik:VisualizationLayer.Reader>
                                <telerik:AsyncShapeFileReader x:Name="mapShapeDataReader" Source="C://Projects//Unified Audience System//Platform Services//Branches//Dev//2014_Q3//MapLibrary//Maps//world_continents.shp" ToolTipFormat="CONTINENT"
                                                    DataSource="C://Projects//Unified Audience System//Platform Services//Branches//Dev//2014_Q3//MapLibrary//Maps//world_continents.dbf"/>
                                <!--<telerik:AsyncShapeFileReader x:Name="mapShapeDataReader" Source="pack://application:,,,/MapLibrary;component/Maps/world_continents.shp" ToolTipFormat="CONTINENT"
                                                    DataSource="pack://application:,,,/MapLibrary;component/Maps/world_continents.shp"/>-->
                            </telerik:VisualizationLayer.Reader>
                        </telerik:VisualizationLayer>
                    </telerik:RadMap>
                </Border>
                <Image x:Name="mapBack" Grid.Column="1" Grid.Row="0" Margin="12,0,0,40" Height="32" Width="32" HorizontalAlignment="Left" VerticalAlignment="Bottom" 
                       Source="/ImageLibrary;Component/Images/32/left_circular-32.png" Visibility="Visible" MouseUp="mapBack_MouseUp" Style="{StaticResource mapArrow}"/>
            </Grid>
        </Grid>
        <Grid x:Name="LogsPage" Grid.Column="0" Grid.Row="0" Grid.RowSpan="2" Grid.ColumnSpan="2" Visibility="Collapsed">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="190"/>
                <ColumnDefinition Width="200"/>
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition />
                <RowDefinition />
            </Grid.RowDefinitions>
            <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Vertical" Margin="15,0,0,0" >
                <TextBlock Text="Filter Options" FontSize="16" />
                <TextBlock Text="Only View Logs With Errors: " Margin="0,50,0,0" HorizontalAlignment="Right"/>
                <TextBlock Text="Only View Logs Without Errors: " Margin="0,10,0,0" HorizontalAlignment="Right"/>
                <TextBlock Text="Clients: " Margin="0,10,0,0" HorizontalAlignment="Right"/>
            </StackPanel>
            <StackPanel Grid.Row="1" Grid.Column="2" Orientation="Vertical" Margin="15,60,0,0">
                <CheckBox x:Name="cBoxErrors" Style="{DynamicResource cbTemplate}" Margin="0,10,0,0" Tag="Errors" Checked="FileLogsCheckUnCheckBox" />
                <CheckBox x:Name="cBoxNoErrors" Style="{DynamicResource cbTemplate}" Margin="0,10,0,0" Tag="NoErrors" Checked="FileLogsCheckUnCheckBox" />
                <telerik:RadComboBox x:Name="rcmbBoxLogClients" Margin="0,10,0,0" ItemTemplate="{StaticResource RadComboCheckBoxesClient}"/>
                <StackPanel Margin="0,10,0,0" Orientation="Horizontal">
                    <telerik:RadButton x:Name="rBtnFilterLogs" Height="19" Width="50" Content="Filter" HorizontalAlignment="Left" Click="rBtnFilterLogs_Click"/>
                    <telerik:RadButton x:Name="rBtnResetLogs" Height="19" Width="50" Margin="10,0,0,0" Content="Reset" HorizontalAlignment="Left" Click="rBtnResetLogs_Click"/>
                </StackPanel>
            </StackPanel>
            <StackPanel Grid.Row="1" Grid.Column="3" Orientation="Vertical" Margin="15,0,0,0">
                <TextBlock Grid.Row="0" Text="Logs List" FontSize="16" FontFamily="Calibri" Margin="15,0,0,0" />
                <telerik:RadTreeView x:Name="rtvLogsList" Grid.Row="1" Margin="8" IsDragDropEnabled="False" SelectionMode="Single" Height="500" MaxHeight="500" ScrollViewer.CanContentScroll="True" 
                                     SelectionChanged="rtvLogsList_SelectionChanged">
                </telerik:RadTreeView>
            </StackPanel>
            <Grid x:Name="gridBackGround" Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="4" Opacity="0" Visibility="Collapsed">
                <Grid.Background>
                    <SolidColorBrush Color="Gray" />
                </Grid.Background>
            </Grid>
            <StackPanel x:Name="spFileLogViewer" Orientation="Horizontal" Grid.ColumnSpan="4" Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" HorizontalAlignment="Center" Margin="30,60,30,30" Visibility="Collapsed">
                <telerik:RadRichTextBox x:Name="richtxtBoxLogs" Grid.ColumnSpan="3" Grid.Row="0" Grid.Column="0" Grid.RowSpan="3"  Height="600" Width="550" VerticalAlignment="Top"
                                    IsSpellCheckingEnabled="False" IsReadOnly="True"/>
                <Border x:Name="logMenu" BorderThickness="1" CornerRadius="0,15,15,0" Width="80" Height="280" Margin="0,0,0,0" BorderBrush="Black" Background="#FFF47E1F" VerticalAlignment="Top">
                    <StackPanel Orientation="Vertical">
                        <Image x:Name="Close" HorizontalAlignment="Center" Margin="15" Width="40" Height="40" Tag="Close" Source="/ImageLibrary;Component/Images/Windows/CloseWhiteX72.png" MouseUp="FileLogMenuClick" Style="{StaticResource mouseOverImage}" ToolTip="Close"/>
                        <Image x:Name="Search" HorizontalAlignment="Center" Margin="15" Width="32" Height="32" Tag="Search" Source="/ImageLibrary;Component/Images/Search/SearchWhite-72.png" MouseUp="FileLogMenuClick" Style="{StaticResource mouseOverImage}" ToolTip="Search"/>
                        <Image x:Name="Save" HorizontalAlignment="Center" Margin="15" Width="32" Height="32" Tag="Save" Source="/ImageLibrary;Component/Images/32/save-32White.png" MouseUp="FileLogMenuClick" Style="{StaticResource mouseOverImage}" ToolTip="Save"/>
                        <Image x:Name="Print" HorizontalAlignment="Center" Margin="15" Width="32" Height="32" Tag="Print" Source="/ImageLibrary;Component/Images/Print/printer-48white.png" MouseUp="FileLogMenuClick" Style="{StaticResource mouseOverImage}" ToolTip="Print"/>
                    </StackPanel>
                </Border>
            </StackPanel>
        </Grid>
        <Rectangle x:Name="swipeRec" Grid.Column="0" Grid.Row="0" Grid.RowSpan="3" VerticalAlignment="Top" HorizontalAlignment="Left" Height="670" Width="40" Fill="#FFF47E1F"/>
        <telerik:RadButton x:Name="rbHome" Tag="Home" Height="40" Width="50" Margin="0,0,0,350" HorizontalAlignment="Left" VerticalAlignment="Center" MouseEnter="RadButton_MouseEnter" MouseLeave="RadButton_MouseLeave" Click="rbHome_Click">
            <ContentControl>
                <Image x:Name="homePage" HorizontalAlignment="Center" Margin="5" Source="/ImageLibrary;Component/Images/32/UAD-HomeWhite.png">
                </Image>
            </ContentControl>
        </telerik:RadButton>
        <StackPanel x:Name="menuButtons" Grid.Column="0" Grid.Row="0" Grid.RowSpan="2" Margin="0,50,0,0" VerticalAlignment="Center" Orientation="Vertical" HorizontalAlignment="Left">
            <telerik:RadButton x:Name="rbReport" Tag="Reports" Height="40" Width="50" Margin="0,0,0,20" HorizontalAlignment="Left" VerticalAlignment="Center" MouseEnter="RadButton_MouseEnter" MouseLeave="RadButton_MouseLeave" Click="rbReport_Click">
                <ContentControl>
                    <Image x:Name="Reports" HorizontalAlignment="Center" Margin="5" Source="/ImageLibrary;Component/Images/32/UAD-ReportWhite.png">
                    </Image>
                </ContentControl>
            </telerik:RadButton>
            <telerik:RadButton x:Name="rbGraph" Tag="Graphs" Height="40" Width="50" Margin="0,0,0,20" HorizontalAlignment="Left" MouseEnter="RadButton_MouseEnter" MouseLeave="RadButton_MouseLeave" Click="rbGraph_Click">
                <ContentControl>
                    <Image x:Name="Graphs" HorizontalAlignment="Center" Margin="5" Source="/ImageLibrary;Component/Images/32/UAD-GraphWhite.png">
                    </Image>
                </ContentControl>
            </telerik:RadButton>
            <telerik:RadButton x:Name="rbLogs" Tag="View Logs" Height="40" Width="50" Margin="0,0,0,20" HorizontalAlignment="Left" MouseEnter="RadButton_MouseEnter" MouseLeave="RadButton_MouseLeave" Click="rbLogs_Click">
                <ContentControl>
                    <Image x:Name="ViewLogs" HorizontalAlignment="Center" Margin="5" Source="/ImageLibrary;Component/Images/32/UAD-LogWhite.png">
                    </Image>
                </ContentControl>
            </telerik:RadButton>
            <!--<telerik:RadButton x:Name="rbStatus" Tag="UAD Status"  Height="40" Width="50" Margin="0,0,0,20" HorizontalAlignment="Left" MouseEnter="RadButton_MouseEnter" MouseLeave="RadButton_MouseLeave">
                <ContentControl>
                    <Image x:Name="UADStatus" HorizontalAlignment="Center" Margin="5" Source="/ImageLibrary;Component/Images/32/UADStatus-32.png">
                    </Image>
                </ContentControl>
            </telerik:RadButton>-->
        </StackPanel>
    </Grid>
</UserControl>