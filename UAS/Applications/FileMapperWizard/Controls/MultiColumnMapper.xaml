<UserControl x:Class="FileMapperWizard.Controls.MultiColumnMapper"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" 
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="545" Width="545">
    <StackPanel Name="spMultiMap" Orientation="Horizontal">        
        <telerik:RadComboBox Name="cbUADMultiMapper" Margin="75,0,0,0" VerticalAlignment="Center" Width="247" Height="23" SelectionChanged="cbUADMultiMapper_SelectionChanged"></telerik:RadComboBox>
        <telerik:RadButton Name="btnDel" ToolTip="Delete Multi Mapping" Margin="2,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="26" Height="24" Click="btnDel_Click">
            <Image Source="/ImageLibrary;Component/Images/Windows/CloseWhiteX72.png" Width="16" Height="16" />
        </telerik:RadButton>        
    </StackPanel>
</UserControl>
